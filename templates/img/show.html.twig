{% extends 'base.html.twig' %}

{% block title %}Img{% endblock %}

{% block body %}
    <h1>Img</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ img.id }}</td>
            </tr>
            <tr>
                <th>FileName</th>
                <td>{{ img.fileName }}</td>
            </tr>
            <tr>
                <th>Alt</th>
                <td>{{ img.alt }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('img_index') }}">back to list</a>

    <a href="{{ path('img_edit', {'id': img.id}) }}">edit</a>

    {{ include('img/_delete_form.html.twig') }}
{% endblock %}
